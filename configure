			{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: no" >&5
printf "%s\n" "no" >&6; }

			{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for tar file" >&5
printf %s "checking for tar file... " >&6; }
			{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: $tarfile" >&5
printf "%s\n" "$tarfile" >&6; }

			{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking if unpacked" >&5
printf %s "checking if unpacked... " >&6; }
			if  test -d $dir
then :

				{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: result: yes" >&5
