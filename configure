#endif"

ac_subst_vars='SHELL PATH_SEPARATOR PACKAGE_NAME PACKAGE_TARNAME PACKAGE_VERSION PACKAGE_STRING PACKAGE_BUGREPORT exec_prefix prefix program_transform_name bindir sbindir libexecdir datadir sysconfdir sharedstatedir localstatedir libdir includedir oldincludedir infodir mandir build_alias host_alias target_alias DEFS ECHO_C ECHO_N ECHO_T LIBS package_copyright package_major package_minor package_build package_version package_string platform MD5SUM ac_ct_MD5SUM AUTOCONF ac_ct_AUTOCONF abs_incdir abs_libdir abs_tardir snert_tardir snert_incdir snert_libdir enable_access_tagless ENABLE_MINGW enable_fork enable_alt_flock LIBSNERT_SQLITE3_DIR LIBSNERT_SQLITE3_VERSION WITH_WINDOWS_SDK FAMILY CC CFLAGS LDFLAGS CPPFLAGS ac_ct_CC EXEEXT OBJEXT CPP EGREP CC_E CC_E_NAME CC_O CC_O_NAME COMPILE ARCHIVE LIBEXT LIBSNERT RANLIB ac_ct_RANLIB TAR_I SET_MAKE ldd_tool ac_ct_ldd_tool HAVE_LIB_RT LIBOBJS HAVE_LIB_PTHREAD HAVE_LIB_SQLITE3 HAVE_LIB_DB HAVE_LIB_WS2_32 HAVE_LIB_IPHLPAPI HAVE_LIB_PAM HAVE_LIB_MILTER SERIALIZATION_API SHARED_MEMORY_API LTLIBOBJS'
ac_subst_files=''

# Initialize some variables set by options.
ac_init_help=
ac_init_version=false
# The variables have the same names as the options, with
# dashes changed to underlines.
