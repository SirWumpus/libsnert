				tarfile=`ls -t1 sqlite*tar.gz | head -n 1`
		dir=`basename $tarfile .tar.gz`

		is_amalgamation=false
		if expr ${dir} : 'sqlite-autoconf-.*' >/dev/null
then :
  is_amalgamation=true
fi
		if ! $is_amalgamation -a expr ${dir} : 'sqlite-amalgamation-.*'
then :
  is_amalgamation=true; i=`echo ${dir} | sed -e 's/amalgamation-//'`; dir=${i}
fi

		LIBSNERT_SQLITE3_VERSION=${dir}


		echo "sqlite directory... $with_sqlite3"
		echo "bundled version..." ${LIBSNERT_SQLITE3_VERSION}
		{ printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for previously built threaded SQLite3" >&5
